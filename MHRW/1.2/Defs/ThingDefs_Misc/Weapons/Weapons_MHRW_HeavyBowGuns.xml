<Defs>

<!--> HEAVY BOWGUNS <-->

	<ThingDef ParentName="BaseHumanMakeableGun">
		<defName>MH_Heavybowgun</defName>
		<label>Heavy Bowgun</label>
		<description>A firearm designed to fire large-calliber shells capable of penetrating the thick hides of monsters. The heavier variant has increased range. \n\n\n\n This one is a basic steel version. \n\n ================== \n\n Ammo type: Normal</description>
		<graphicData>
			<texPath>Things/Item/Equipment/WeaponRanged/MHHeavybowgun</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>2.2</drawSize>
		</graphicData>
		<comps>
			<li Class="CompOversizedWeapon.CompProperties_OversizedWeapon">
				<eastOffset>(0.1,0,0)</eastOffset>
				<westOffset>(-0.1,0,0)</westOffset>
				<southOffset>(0,0,0)</southOffset>
				<northOffset>(0,0,0)</northOffset>
			</li>
		</comps>
		<soundInteract>Interact_Rifle</soundInteract>
		<recipeMaker>
			<researchPrerequisite>MHBowgunTier1</researchPrerequisite>
			<skillRequirements>
				<Crafting>7</Crafting>
			</skillRequirements>
			<recipeUsers>
                <li>MHSmithy</li>
			</recipeUsers>
		</recipeMaker>
		<statBases>
			<WorkToMake>40000</WorkToMake>
			<Mass>3.5</Mass>
			<AccuracyTouch>0.40</AccuracyTouch>
			<AccuracyShort>0.65</AccuracyShort>
			<AccuracyMedium>0.70</AccuracyMedium>
			<AccuracyLong>0.65</AccuracyLong>
			<RangedWeapon_Cooldown>1.20</RangedWeapon_Cooldown>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>-0.3</MoveSpeed>
		</equippedStatOffsets>
		<equippedAngleOffset>0</equippedAngleOffset>
		<costList>
			<Steel>100</Steel>
			<ComponentIndustrial>4</ComponentIndustrial>
		</costList>
		<verbs>
			<li>
				<verbClass>Verb_Shoot</verbClass>
				<hasStandardCommand>true</hasStandardCommand>
				<defaultProjectile>Bullet_MHHBGNormalOre</defaultProjectile>
				<warmupTime>3.0</warmupTime>
				<range>40.9</range>
				<burstShotCount>1</burstShotCount>
				<ticksBetweenBurstShots>20</ticksBetweenBurstShots>
				<soundCast>Shot_BowgunNormal</soundCast>
				<soundCastTail>GunTail_Medium</soundCastTail>
				<muzzleFlashScale>9</muzzleFlashScale>
			</li>
		</verbs>
		<tools>
			<li>
				<label>stock</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>5</power>
				<cooldownTime>2</cooldownTime>
			</li>
			<li>
				<label>barrel</label>
				<capacities>
					<li>Blunt</li>
					<li>Poke</li>
				</capacities>
				<power>5</power>
				<cooldownTime>2</cooldownTime>
			</li>
		</tools>
	</ThingDef>
	
	<ThingDef ParentName="BaseBullet">
		<defName>Bullet_MHHBGNormalOre</defName>
