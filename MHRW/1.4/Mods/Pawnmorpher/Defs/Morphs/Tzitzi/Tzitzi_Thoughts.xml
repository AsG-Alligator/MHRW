<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Schemas/ThoughtDef.xsd">
	<ThoughtDef ParentName="OutlookMemoryBase">
        <durationDays>1</durationDays>
        <defName>EtherTzitziSnout</defName>
        <stages>
            <li>
                <label>Tzitzi face</label>
                <description>These teeth are scary.</description>
                <visible>true</visible>
                <baseMoodEffect>-1</baseMoodEffect>
            </li>
            <li>
                <label>Tzitzi face</label>
                <description>I look so cool! I can make so many fun sounds now too.</description>
                <visible>true</visible>
                <baseMoodEffect>3</baseMoodEffect>
            </li>
            <li>
                <label>Tzitzi face</label>
                <description>My face!</description>
                <visible>true</visible>
                <baseMoodEffect>-3</baseMoodEffect>
            </li>
        </stages>
		</ThoughtDef>
		<ThoughtDef ParentName="OutlookMemoryBase">
        <durationDays>1</durationDays>
        <defName>EtherTzitziFlaps</defName>
        <stages>
            <li>
                <label>Tzitzi flashflaps</label>
                <description>What the hell are these for?</description>
                <visible>true</visible>
                <baseMoodEffect>-1</baseMoodEffect>
            </li>
            <li>
                <label>Tzitzi flashflaps</label>
                <description>Say cheese!</description>
                <visible>true</visible>
                <baseMoodEffect>2</baseMoodEffect>
            </li>
            <li>
                <label>Tzitzi flashflaps</label>
                <description>Ah! My eyes! I blinded myself!</description>
                <visible>true</visible>
                <baseMoodEffect>-2</baseMoodEffect>
            </li>
        </stages>
    </ThoughtDef>
    <ThoughtDef ParentName="OutlookMemoryBase">
        <durationDays>1</durationDays>
        <defName>EtherTzitziTail</defName>
        <stages>
            <li>
                <label>Tzitzi tail</label>
                <description>I keep bumping into stuff with it.</description>
                <visible>true</visible>
                <baseMoodEffect>-1</baseMoodEffect>
            </li>
            <li>
                <label>Tzitzi tail</label>
                <description>It helps with my balance a lot. I really like that pattern too.</description>
                <visible>true</visible>
                <baseMoodEffect>3</baseMoodEffect>
            </li>
            <li>
                <label>Tzitzi tail</label>
                <description>Gah! Stupid tail!</description>
                <visible>true</visible>
                <baseMoodEffect>-3</baseMoodEffect>
            </li>
        </stages>
    </ThoughtDef>
</Defs>