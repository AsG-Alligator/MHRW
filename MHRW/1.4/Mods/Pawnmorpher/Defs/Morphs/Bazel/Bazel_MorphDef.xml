<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MorphDef.xsd">
	<Pawnmorph.MorphDef ParentName="SensitiveFoodMorph">
		<defName>BazelMorph</defName>
		<label>bazelmorph</label>
		<description>A wyvern-shaped morph with Bazel features.</description>
		<race>Bazelgeuse</race>
		<classification>MHWyverns</classification>
		<categories>
			<li>Powerful</li>
			<li>Monster</li>
		</categories>
		<fullTransformation>PawnmorphBazelTF</fullTransformation>
		<partialTransformation>PawnmorphBazelTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(90, 64, 68)</skinColorOverride>
				<hairColorOverride>(0,0,0,0)</hairColorOverride>
			</graphicsSettings>
		</raceSettings>
		<transformSettings>
			<transformationMemory>BazelTransformationMemory</transformationMemory>
			<revertedMemory>BazelRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>
