<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Schemas/ThoughtDef.xsd">
	<ThoughtDef ParentName="OutlookMemoryBase">
        <durationDays>1</durationDays>
        <defName>EtherGendromeSnout</defName>
        <stages>
            <li>
                <label>Gendrome face</label>
                <description>These teeth look scary.</description>
                <visible>true</visible>
                <baseMoodEffect>-1</baseMoodEffect>
            </li>
            <li>
                <label>Gendrome face</label>
                <description>Fearsome fangs!</description>
                <visible>true</visible>
                <baseMoodEffect>3</baseMoodEffect>
            </li>
            <li>
                <label>Gendrome face</label>
                <description>I look hideous.</description>
                <visible>true</visible>
                <baseMoodEffect>-3</baseMoodEffect>
            </li>
        </stages>
		</ThoughtDef>
		<ThoughtDef ParentName="OutlookMemoryBase">
        <durationDays>1</durationDays>
        <defName>EtherGendromeCrest</defName>
        <stages>
            <li>
                <label>Gendrome crest</label>
                <description>Well, this will complicate wearing hats.</description>
                <visible>true</visible>
                <baseMoodEffect>-1</baseMoodEffect>
            </li>
            <li>
                <label>Gendrome crest</label>
                <description>I think it fits my style.</description>
                <visible>true</visible>
                <baseMoodEffect>2</baseMoodEffect>
            </li>
            <li>
                <label>Gendrome crest</label>
                <description>Stupid goddamn crests, they are so annoying.</description>
                <visible>true</visible>
                <baseMoodEffect>-2</baseMoodEffect>
            </li>
        </stages>
    </ThoughtDef>
    <ThoughtDef ParentName="OutlookMemoryBase">
        <durationDays>1</durationDays>
        <defName>EtherGendromeTail</defName>
        <stages>
            <li>
                <label>Gendrome tail</label>
                <description>I keep bumping into stuff with it.</description>
                <visible>true</visible>
                <baseMoodEffect>-1</baseMoodEffect>
            </li>
            <li>
                <label>Gendrome tail</label>
                <description>It helps with my balance a lot. I really like that pattern too.</description>
                <visible>true</visible>
                <baseMoodEffect>3</baseMoodEffect>
            </li>
            <li>
                <label>Gendrome tail</label>
                <description>Gah! Stupid tail!</description>
                <visible>true</visible>
                <baseMoodEffect>-3</baseMoodEffect>
            </li>
