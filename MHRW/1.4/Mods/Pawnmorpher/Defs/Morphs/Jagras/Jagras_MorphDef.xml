<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MorphDef.xsd">
	<Pawnmorph.MorphDef ParentName="SensitiveFoodMorph">
		<defName>JagrasMorph</defName>
		<label>jagrasmorph</label>
		<description>A scaly reptile morph with traits similar to that of a Great Jagras.</description>
		<race>GreatJagras</race>
		<classification>MHDrakes</classification>
		<categories>
			<li>Powerful</li>
			<li>Monster</li>
		</categories>
		<fullTransformation>PawnmorphJagrasTF</fullTransformation>
		<partialTransformation>PawnmorphJagrasTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(211, 188, 95)</skinColorOverride>
			</graphicsSettings>
		</raceSettings>
		<transformSettings>
			<transformationMemory>JagrasTransformationMemory</transformationMemory>
			<revertedMemory>JagrasRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>