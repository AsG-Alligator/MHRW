<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MorphDef.xsd">
	<Pawnmorph.MorphDef ParentName="SensitiveFoodMorph">
		<defName>VelocidromeMorph</defName>
		<label>velocimorph</label>
		<description>A scaly reptile morph with traits similar to that of a Velocidrome.</description>
		<race>Velociprey</race>
		<classification>MHRaptors</classification>
		<categories>
			<li>Arid</li>
			<li>Reptile</li>
		</categories>
		<fullTransformation>PawnmorphVelocidromeTF</fullTransformation>
		<partialTransformation>PawnmorphVelocidromeTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(65, 126, 166)</skinColorOverride>
			</graphicsSettings>
		</raceSettings>
		<transformSettings>
			<transformationMemory>VelocidromeTransformationMemory</transformationMemory>
			<revertedMemory>VelocidromeRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>