		<fullTransformation>PawnmorphApcerosTF</fullTransformation>
		<partialTransformation>PawnmorphApcerosTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(95, 59,40)</skinColorOverride>
			</graphicsSettings>
		</raceSettings>
		<transformSettings>
			<transformationMemory>ApcerosTransformationMemory</transformationMemory>
			<revertedMemory>ApcerosRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>