<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MorphDef.xsd">
	<Pawnmorph.MorphDef ParentName="SensitiveFoodMorph">
		<defName>GirrosMorph</defName>
		<label>girrosmorph</label>
		<description>A scaly reptile morph with traits similar to that of a Girros.</description>
		<race>Girros</race>
		<classification>MHDrakes</classification>
		<categories>
			<li>Powerful</li>
			<li>Monster</li>
		</categories>
		<fullTransformation>PawnmorphGirrosTF</fullTransformation>
		<partialTransformation>PawnmorphGirrosTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(36,43,35)</skinColorOverride>
			</graphicsSettings>
		</raceSettings>
		<transformSettings>
			<transformationMemory>GirrosTransformationMemory</transformationMemory>
			<revertedMemory>GirrosRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>