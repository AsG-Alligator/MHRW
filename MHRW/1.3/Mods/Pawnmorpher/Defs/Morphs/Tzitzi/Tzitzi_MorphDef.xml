		<fullTransformation>PawnmorphTzitziTF</fullTransformation>
		<partialTransformation>PawnmorphTzitziTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(72, 68, 96)</skinColorOverride>
			</graphicsSettings>
		</raceSettings>
		<transformSettings>
			<transformationMemory>TzitziTransformationMemory</transformationMemory>
			<revertedMemory>TzitziRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
