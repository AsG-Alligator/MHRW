<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Schemas/ThoughtDef.xsd">
	<ThoughtDef ParentName="OutlookMemoryBase">
        <durationDays>1</durationDays>
        <defName>EtherKushalaSnout</defName>
        <stages>
            <li>
                <label>Kushala snout</label>
                <description>I dont appreciate the constant metallic taste in my mouth.</description>
                <visible>true</visible>
                <baseMoodEffect>-1</baseMoodEffect>
            </li>
            <li>
                <label>Kushala snout</label>
                <description>ROOAAR!!</description>
                <visible>true</visible>
                <baseMoodEffect>3</baseMoodEffect>
            </li>
            <li>
                <label>Kushala snout</label>
                <description>I feel like I have a metal bucket stuck to my face.</description>
                <visible>true</visible>
                <baseMoodEffect>-3</baseMoodEffect>
            </li>
        </stages>
		</ThoughtDef>
