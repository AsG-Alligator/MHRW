<Defs>  

	<MVCF.ModDef>
	<defName>MHRW_MonsterHunterRimworld</defName>
	<label>Monster Hunter Rimworld</label>
	<description>Adding animal ranged attacks for Monster Hunter Rimworld creatures</description>
	<Features>
		<RangedAnimals>true</RangedAnimals>
		<HediffVerbs>true</HediffVerbs>
		<IntegratedToggle>true</IntegratedToggle>
	</Features>
	</MVCF.ModDef>
	
	
  <ThingDef ParentName="AnimalThingBase">
    <defName>KuluYaKu</defName>
    <label>kulu ya-ku</label>
    <description>An odd bird wyvern that has developed limbs capable of carrying weapons. It's been spotted stealing eggs from nests of other animals.</description>
    <statBases>
      <MoveSpeed>5</MoveSpeed>
      <MarketValue>2950</MarketValue>
      <ComfyTemperatureMin>-5</ComfyTemperatureMin>
	  <ComfyTemperatureMax>60</ComfyTemperatureMax>
	  <ArmorRating_Blunt>0.2</ArmorRating_Blunt>
	  <ArmorRating_Sharp>0.2</ArmorRating_Sharp>
    </statBases>
	<modExtensions>
			<li Class="AnimalBehaviours.AnimalStatExtension">

				<showImageInInfoCard>true</showImageInInfoCard>
				<ImageToShowInInfoCard>Things/Art/ArtKulu</ImageToShowInInfoCard>

			</li>
	</modExtensions>
	<comps>
      <li Class="CompProperties_EggLayer">
        <eggFertilizedDef>EggKuluFertilized</eggFertilizedDef>
        <eggFertilizationCountMax>1</eggFertilizationCountMax>
        <eggLayIntervalDays>30</eggLayIntervalDays>
        <eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
        <eggCountRange>1</eggCountRange>
      </li>
    </comps>
    <tools>
      <li>
        <label>left claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>10</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>FrontLeftClaws</linkedBodyPartsGroup>
      </li>
      <li>
        <label>right claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>10</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>FrontRightClaws</linkedBodyPartsGroup>
      </li>
      <li>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>15</power>
        <cooldownTime>2.6</cooldownTime>
        <linkedBodyPartsGroup>Beak</linkedBodyPartsGroup>
        <chanceFactor>0.7</chanceFactor>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>15</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <race>
      <body>MHBruteDromeWithBeak</body>
      <baseHungerRate>0.3</baseHungerRate>
      <baseBodySize>3.0</baseBodySize>
      <baseHealthScale>4.6</baseHealthScale>
      <foodType>VegetarianRoughAnimal, OvivoreAnimal</foodType>
      <leatherDef>Leather_KuluYaKu</leatherDef>
      <gestationPeriodDays>55</gestationPeriodDays>
      <wildness>0.5</wildness>
      <trainability>Advanced</trainability>
      <nuzzleMtbHours>120</nuzzleMtbHours>
      <lifeExpectancy>120</lifeExpectancy>
      <manhunterOnTameFailChance>0.10</manhunterOnTameFailChance>
      <manhunterOnDamageChance>0.80</manhunterOnDamageChance>
	  <wildBiomes>
		<TemperateForest>0.01</TemperateForest>
		<TemperateSwamp>0.04</TemperateSwamp>
		<BorealForest>0</BorealForest>
		<Tundra>0</Tundra>
		<ColdBog>0</ColdBog>
		<IceSheet>0</IceSheet>
		<TropicalRainforest>0.2</TropicalRainforest>
		<TropicalSwamp>0.2</TropicalSwamp>
		<AridShrubland>0.2</AridShrubland>
		<Desert>0.2</Desert>
		<ExtremeDesert>0.05</ExtremeDesert>
	  </wildBiomes>
      <lifeStageAges>
        <li>
          <def>AnimalBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>1.0</minAge>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>3.0</minAge>
          <soundWounded>Pawn_Thrumbo_Pain</soundWounded>
          <soundDeath>Pawn_Thrumbo_Death</soundDeath>
          <soundCall>Pawn_Thrumbo_Call</soundCall>
          <soundAngry>Pawn_Thrumbo_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
    </race>
    <tradeTags>
      <li>AnimalRare</li>
      <li>AnimalFighter</li>
    </tradeTags>
  </ThingDef>
  
